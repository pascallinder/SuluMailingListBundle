<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>filtered_contacts</key>
    <properties>
        <property name="id" visibility="no">
            <field-name>id</field-name>
            <entity-name>Sulu\Bundle\ContactBundle\Entity\Contact</entity-name>
        </property>
        <property name="firstName" visibility="yes">
            <field-name>firstName</field-name>
            <entity-name>Sulu\Bundle\ContactBundle\Entity\Contact</entity-name>
        </property>

        <property name="lastName" visibility="yes">
            <field-name>lastName</field-name>
            <entity-name>Sulu\Bundle\ContactBundle\Entity\Contact</entity-name>
        </property>

        <property name="mainEmail" visibility="yes">
            <field-name>mainEmail</field-name>
            <entity-name>Sulu\Bundle\ContactBundle\Entity\Contact</entity-name>
        </property>
    </properties>
</list>
